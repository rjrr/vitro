<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="EnableNodesResp" type="EnableNodesRespType"/>
    <xs:complexType name="EnableNodesRespType">
        <xs:sequence>
            <xs:element type="xs:string" name="message-type"/>
            <xs:element type="xs:string" name="vgwId"/>
            <xs:element type="confirmedEnabledNodesListType" name="confirmedEnabledNodesList"/>
            <xs:element type="xs:string" name="timestamp"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="confirmedEnabledNodesListType">
        <xs:sequence>
            <xs:element type="confirmedEnabledNodesListItemType" name="confirmedEnabledNodesListItem" maxOccurs="unbounded" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="confirmedEnabledNodesListItemType">
        <xs:sequence>
            <xs:element type="xs:string" name="nodeId"/>
            <xs:element type="xs:string" name="status"/>
            <xs:element type="xs:string" name="ofRemoteTimestamp"/>
            <xs:element type="xs:string" name="gwInitFlag" />
        </xs:sequence>
    </xs:complexType>
</xs:schema>
